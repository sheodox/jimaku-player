{
  "name": "jimaku-player",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-subtitler:prod": "vite build -c vite-subtitler.config.js ",
    "build-subtitler:release": "npm run build-subtitler:prod && node dist.js release",
    "build-subtitler:test": "npm run build-subtitler:prod && node dist.js",
    "build-static:prod": "vite build",
    "build-static:dev": "vite",
    "build-server:dev": "tsc --watch",
    "build-server:prod": "tsc",
    "build:prod": "npm run build-static:prod && npm run build-server:prod",
    "lint": "npm run lint-server:prod && npm run lint-static:prod",
    "lint-server:prod": "eslint 'src/server/**/*.ts' && tsc --noEmit",
    "lint-static:dev": "svelte-check --watch",
    "lint-static:prod": "eslint 'src/static/**/*.{js,ts,svelte}' && svelte-check",
    "server:dev": "nodemon build/server/server.js --watch build/server",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "test": "npm run style:check && npm run lint && jest",
    "test:dev": "jest --watch",
    "prepare": "husky install"
  },
  "author": "sheodox",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@types/page": "^1.11.5",
    "express": "^4.17.1",
    "glob": "^9.2.0",
    "nanoid": "^4.0.1",
    "page": "^1.11.6",
    "pug": "^3.0.2",
    "sass": "^1.58.3",
    "sharp": "^0.31.3",
    "sheodox-ui": "^0.18.30",
    "svelte": "^3.44.3",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.4",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "vite": "^4.1.4"
  }
}
