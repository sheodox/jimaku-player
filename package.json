{
  "name": "jimaku-player",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-subtitler:prod": "vite build -c vite-subtitler.config.js ",
    "build-subtitler:release": "npm run build-subtitler:prod && node dist.js release",
    "build-subtitler:test": "npm run build-subtitler:prod && node dist.js",
    "build-static:prod": "vite build",
    "build-static:dev": "vite",
    "build-server:dev": "tsc --watch",
    "build-server:prod": "tsc",
    "build:prod": "npm run build-static:prod && npm run build-server:prod",
    "lint": "npm run lint-server:prod && npm run lint-static:prod",
    "lint-server:prod": "eslint 'src/server/**/*.ts' && tsc --noEmit",
    "lint-static:dev": "svelte-check --watch",
    "lint-static:prod": "eslint 'src/static/**/*.{js,ts,svelte}' && svelte-check",
    "server:dev": "nodemon build/server/server.js --watch build/server",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "test": "npm run style:check && npm run lint && jest",
    "test:dev": "jest --watch",
    "prepare": "husky install"
  },
  "author": "sheodox",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@types/page": "^1.11.5",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "nanoid": "^3.1.30",
    "page": "^1.11.5",
    "pug": "^3.0.2",
    "sass": "^1.45.2",
    "sharp": "^0.29.3",
    "sheodox-ui": "^0.16.7",
    "svelte": "^3.49.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.33",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.6",
    "@types/sharp": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.6",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "vite": "^2.7.10"
  }
}
